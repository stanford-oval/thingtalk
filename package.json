{
  "name": "thingtalk",
  "version": "1.11.0",
  "description": "ThingTalk Just-In-Time Compiler, Library and Runtime",
  "license": "Apache-2.0",
  "repository": "https://github.com/stanford-oval/thingtalk",
  "author": "Stanford Open Virtual Assistant Lab",
  "contributors": [
    {
      "name": "Giovanni Campagna",
      "email": "gcampagn@cs.stanford.edu"
    },
    {
      "name": "Silei Xu",
      "email": "silei@cs.stanford.edu"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "byline": "^5.0.0",
    "consumer-queue": "^1.0.0",
    "semver": "^7.3.2",
    "smtlib": "^0.1.1",
    "string-interp": "^0.3.1",
    "thingtalk-units": "^0.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "coveralls": "^3.0.0",
    "deep-equal": "^2.0.1",
    "eslint": "^7.1.0",
    "nyc": "^15.0.0",
    "pegjs": "~0.10.0",
    "q": "^1.5.1",
    "seedrandom": "^3.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "update-pot": "xgettext -f po/POTFILES -x po/POTFILES.skip -o po/${npm_package_name}.pot --from-code UTF-8 --package-name ${npm_package_name} --package-version ${npm_package_version}",
    "update-po": "for i in po/*.po ; do msgmerge -U $i po/${npm_package_name}.pot ; done",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./lib ./tools",
    "test": "nyc ts-node ./test/test_all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "make",
    "doc": "typedoc lib/index.ts"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "lib/grammar.js",
      "test/**",
      "coverage/**"
    ]
  }
}
